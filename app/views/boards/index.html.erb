<div class="container">
  <h1>Dispatch Board</h1>
  <hr>
  <div class="dispatch-table">
  <table class="table table-striped table hover">
    <tr>
      <th>Rider/Driver</th>
      <th>Origin Address</th>
      <th>Destination Address</th>
      <th>Service Level</th>
    </tr>
    <% @jobs.each do |job| %>
    <tr>
    <td>
      
    </td>
    <td><%= job.job_originaddress %></td>
    <td><%= job.job_destinationaddress %></td>
    <td><%= job.job_servicelevel %></td>
    </tr>
    <% end %>
  </table>
</div>
</div>
